<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_pos_form_point_of_sale_inherit" model="ir.ui.view">
            <field name="name">pos.order.view.form.inherit</field>
            <field name="model">pos.order</field>
            <field name="inherit_id" ref="point_of_sale.view_pos_pos_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='session_move_id']" position="after">
                    <field name="account_report" invisible="1"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
